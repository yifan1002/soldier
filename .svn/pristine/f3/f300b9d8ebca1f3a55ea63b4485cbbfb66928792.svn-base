export default {
	state: {
		menuCurrent: 'home',
		subMenuCurrent: '',
		token: '',
	},
	mutations: {
		changeMenu: (state, currentName) => state.menuCurrent = currentName,
		changeSubMenu: (state, currentName) => state.subMenuCurrent = currentName,
		changeToken: (state, val) => state.token = val
	},
}